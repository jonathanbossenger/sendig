{
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "League Spartan",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/league-spartan_normal_300.ttf"
							]
						},
						{
							"fontFamily": "League Spartan",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/league-spartan_normal_400.ttf"
							]
						},
						{
							"fontFamily": "League Spartan",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/league-spartan_normal_500.ttf"
							]
						},
						{
							"fontFamily": "League Spartan",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/league-spartan_normal_700.ttf"
							]
						},
						{
							"fontFamily": "League Spartan",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/league-spartan_normal_900.ttf"
							]
						}
					],
					"fontFamily": "League Spartan",
					"slug": "league-spartan"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var:preset|font-family|league-spartan"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "5rem",
					"fontStyle": "normal",
					"fontWeight": "900",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "3.25rem",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.1"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "2.25rem",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.1"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "1.75rem",
					"fontStyle": "normal",
					"fontWeight": "900",
					"lineHeight": "1.1"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1.5rem",
					"fontStyle": "normal",
					"fontWeight": "900",
					"lineHeight": "1.1"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|league-spartan",
					"lineHeight": "1.1"
				}
			},
			"link": {
				"typography": {
					"fontFamily": "var:preset|font-family|league-spartan"
				}
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|league-spartan",
			"fontSize": "1.3rem",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.2/theme.json"
}